{
  "name": "@argdown/sandbox",
  "version": "1.7.10",
  "private": true,
  "description": "An Argdown editor that runs in the browser",
  "author": "Christian Voigt <1pxsolidblack@googlemail.com>",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "echo \"No tests to run here\"",
    "lint": "eslint src",
    "copy-examples": "rimraf \"./public/examples\" && copyfiles -f \"../../examples/*.argdown\" ./public/examples"
  },
  "dependencies": {
    "@argdown/codemirror-mode": "workspace:^",
    "@argdown/core": "workspace:^",
    "@argdown/map-views": "workspace:^",
    "codemirror": "^5.65.19",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.2.1",
    "vue": "^3.4.28",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.72.0",
    "vite": "^7.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "sideEffects": [
    "./src/main.js",
    "./src/store.js",
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "keywords": [
    "argumentation",
    "argument mapping",
    "Argdown",
    "Editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/argdown/argdown/tree/master/packages/argdown-sandbox"
  },
  "engines": {
    "node": ">= 22.11.0"
  },
  "packageManager": "yarn@4.9.4"
}
